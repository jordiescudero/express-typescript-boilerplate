#
# APPLICATION
#
APP_NAME=bloomen-typescript-boilerplate
APP_SCHEMA=http
APP_HOST=localhost
APP_PORT=3000
APP_ROUTE_PREFIX=/api
APP_BANNER=false

#
# LOGGING
#
LOG_LEVEL=none
LOG_JSON=true
LOG_OUTPUT=dev

#
# Swagger
#
